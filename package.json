{
  "name": "examples-trpc-next-prisma-starter",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "prisma-studio": "prisma studio",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate dev reset",
    "dx:next": "run-s migrate-dev db-seed && next dev",
    "dx:prisma-studio": "pnpm prisma-studio",
    "dx": "run-p dx:* --print-label",
    "dev": "pnpm dx:next",
    "prebuild": "run-s generate migrate",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" src",
    "lint-fix": "pnpm lint --fix",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test-unit": "vitest",
    "test-e2e": "playwright test",
    "test-start": "run-s test-unit test-e2e",
    "postinstall": "pnpm generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@planetscale/database": "^1.19.0",
    "@prisma/client": "^5.19.1",
    "@tanstack/react-query": "^5.55.4",
    "@trpc/client": "11.0.0-rc.502",
    "@trpc/next": "11.0.0-rc.502",
    "@trpc/react-query": "11.0.0-rc.502",
    "@trpc/server": "11.0.0-rc.502",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.33.0",
    "next": "^14.2.10",
    "next-auth": "^4.24.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "superjson": "^2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.0",
    "@testcontainers/postgresql": "^10.13.1",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "eslint": "^9.10.0",
    "eslint-config-next": "^14.2.10",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "prisma": "^5.19.1",
    "start-server-and-test": "^2.0.7",
    "tailwindcss": "^3.4.11",
    "testcontainers": "^10.13.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.4",
    "vitest": "^2.0.5"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
